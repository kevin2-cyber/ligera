<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@android:anim/accelerate_decelerate_interpolator">
    <translate
        android:duration="400"
        android:fromXDelta="0%"
        android:repeatCount="0"
        android:repeatMode="restart"
        android:toXDelta="0%"
        android:valueFrom="0"
        android:valueTo="0"
        android:valueType="floatType">
        <propertyValuesHolder
            android:propertyName="translationX"
            android:valueType="floatType">
            <keyframe android:fraction="0" android:value="0" />
            <keyframe android:fraction="0.1" android:value="-10" />
            <keyframe android:fraction="0.2" android:value="10" />
            <keyframe android:fraction="0.3" android:value="-8" />
            <keyframe android:fraction="0.4" android:value="8" />
            <keyframe android:fraction="0.5" android:value="-5" />
            <keyframe android:fraction="0.6" android:value="5" />
            <keyframe android:fraction="0.7" android:value="-2" />
            <keyframe android:fraction="0.8" android:value="2" />
            <keyframe android:fraction="0.9" android:value="-1" />
            <keyframe android:fraction="1.0" android:value="0" />
        </propertyValuesHolder>
    </translate>
</set>

